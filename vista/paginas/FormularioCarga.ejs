<%- include("header"); %>


    <div class="container-fluid">

        <div class="row">
            <div class="col-md-12">

                <div class="col-md-3">

                    <div class="form-group row">

                        <form id="formulario" enctype="multipart/form-data" method="POST">


                            <h2> Formulario Pruebas</h2>
                            <label for="number">Seleccione el Codigo IPS</label>
                            <select class="form-select form-select-lg mb-3" aria-label=".form-select-lg example">

                                <option selected>Seleccionar IPS</option>
                                <% listaIps.forEach(lista=> { %>


                                    <option value="<%= lista.idips %>">
                                        <%= lista.codigo_ips %>
                                    </option>

                                    <% })%>

                            </select>

                            <p>&nbsp;&nbsp;&nbsp;</p>
                            <label for="fecha_ini">Seleccione Rango Fecha</label>
                            <div>
                                <input type="text" name="daterange" value="01/01/2018 - 01/15/2018" />

                                <script>
                                    $(function() {
                                        $('input[name="daterange"]').daterangepicker({
                                            opens: 'left'
                                        }, function(start, end, label) {
                                            console.log("A new date selection was made: " + start.format('YYYY-MM-DD') + ' to ' + end.format('YYYY-MM-DD'));
                                        });
                                    });
                                </script>
                            </div>
                    </div>

                    <div>
                        <button>Consultar Registros</button>
                    </div>



                    <input type="file" name="files" id="files" multiple disabled="disabled">
                    <p>&nbsp;</p>
                    <button class="btn btn-primary btn-md" id="btn-subir" name="btn-subir" disabled="disabled">Subir</button>

                    <div class="preview">
                        <div id="datos"></div>
                    </div>


                    </form>
                </div>

            </div>
        </div>

    </div>

    <%- include("footer"); %>