                                                    </label>

                                                    <%}else{%>
                                                        &nbsp;&nbsp;
                                                        <%}%>

                                                            <%if(item[0].estado==false || item[0].calificacion==true
                                                                ){%>



                                                                <button
                                                                    @click="openModalNew('/form-ctm-registro-indicador/<%= item[0].id_registroindicador %>/?respuesta=1','xl')"
