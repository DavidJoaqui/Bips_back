<div id="container-tab">

</div>

<script>
    const tabActiveClass = "text-gray-600 py-4 px-6 block hover:text-blue-500 focus:outline-none text-blue-500 border-b-2 font-medium border-blue-500 border-l border-t border-r"
    const tabClass = "text-gray-600 py-4 px-6 block hover:text-blue-500 focus:outline-none"

    function loadTab(btn, url) {
        toggleTab(btn, url);
        loadContentHtml(url, '#container-tab')
    }


    function toggleTab(btn, url) {
        const button = $(btn);
        $("#tabs-base button").each(function (index) {
            $(this).removeClass().addClass(tabClass);
            if ($(this).text() === button.text()) {
                button.removeClass().addClass(tabActiveClass);
            }
        });
    }

    loadContentHtml('/ctm-plan-general', '#container-tab')
</script>