<form  id="formSaveEntity">
   
    <label class="block mt-4 text-sm">
        <span class="text-gray-700 dark:text-gray-400">
            Código de entidad
        </span>
        <input  name="cod_entidad" class="block w-full mt-1 text-sm dark:text-gray-300 dark:border-gray-600 dark:bg-gray-700 focus:border-purple-400 focus:outline-none focus:shadow-outline-purple dark:focus:shadow-outline-gray form-input" placeholder="Código de entidad">
      </label>


      <label class="block mt-4 text-sm">
        <span class="text-gray-700 dark:text-gray-400">
            Nombre de entidad
        </span>
        <input  name="nombre_entidad"   class="block w-full mt-1 text-sm dark:text-gray-300 dark:border-gray-600 dark:bg-gray-700 focus:border-purple-400 focus:outline-none focus:shadow-outline-purple dark:focus:shadow-outline-gray form-input" placeholder="Nombre de entidad">
      </label>

      <label class="block mt-4 text-sm">
        <span class="text-gray-700 dark:text-gray-400">
            Regimen que aplica a la entidad:
        </span>
        <select  name="tipo_reg" class="block w-full mt-1 text-sm dark:text-gray-300 dark:border-gray-600 dark:bg-gray-700 form-select focus:border-purple-400 focus:outline-none focus:shadow-outline-purple dark:focus:shadow-outline-gray">
            <option value="C">Contributivo</option>
            <option value="S">Subsidiado</option>
            <option value="V">Vinculado</option>
            <option value="E">Especial</option>
            <option value="O">Otro</option>
        </select>
      </label>
    <button hidden id="btnForm" type="submit"></button>
</form>

<script>

const form = document.querySelector("#formSaveEntity");
form.addEventListener("submit", function (event) {
	// Stop form submission
	event.preventDefault();
    const data=Object.fromEntries(new FormData(form));
    createdAjax('/persistir-entidad',data)

});
</script>