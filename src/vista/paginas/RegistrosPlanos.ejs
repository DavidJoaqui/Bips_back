<%- include("head"); %>
    <%- include("header"); %>

    <%- include("styles_gral"); %>
        <div class="container-fluid" id="lista_reg-planos" name="lista_reg-planos">

            <div class="container" style="margin-top:75px;padding-top: 10px;">

                <div class="col-12">
                    <h1 style="color: grey;">Registros en bodega</h1>

                    <table class="table table-bordered table-striped">
                        <thead style="background-color: cadetblue;
                        color: #fff;">
                            <tr>
                                <th>Codigo IPS</th>
                                <th>Fecha inicial</th>
                                <th>Fecha final</th>
                                <th>Acciones</th>

                            </tr>
                        </thead>
                        <tbody>
                            <% registroplanos.forEach(registroplanos => { %>
                                <tr>
                                    <td>
                                        <%= registroplanos.ips %>
                                    </td>
                                    <td>
                                        <%= registroplanos.fecha_inicio.toLocaleDateString() %>
                                    </td>
                                    <td>
                                        <%= registroplanos.fecha_final.toLocaleDateString()  %>
                                    </td>
                                    <td>


                                        <a href="#" class="btn btn-primary">
                        Eliminar
                    </a>
                                    </td>
                                </tr>
                                <% })%>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>

        <%- include("footer"); %>